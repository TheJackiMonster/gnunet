[pils]
START_ON_DEMAND = @START_ON_DEMAND@
IMMEDIATE_START = YES
HOSTNAME = localhost
BINARY = gnunet-service-pils
ACCEPT_FROM = 127.0.0.1;
ACCEPT_FROM6 = ::1;
UNIXPATH = $GNUNET_RUNTIME_DIR/gnunet-service-pils.sock
UNIX_MATCH_UID = NO
UNIX_MATCH_GID = YES
SECRET_KEY_FILE = $GNUNET_DATA_HOME/pils_key.bin
