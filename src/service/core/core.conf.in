[core]
START_ON_DEMAND = @START_ON_DEMAND@
HOSTNAME = localhost
BINARY = gnunet-service-core
## The device class might go into a more general section than core
## valid values: UNKNOWN, UNWILLING, MOBILE, DESKTOP, ROUTER, SERVER
CLASS = UNKNOWN
ACCEPT_FROM = 127.0.0.1;
ACCEPT_FROM6 = ::1;
UNIXPATH = $GNUNET_RUNTIME_DIR/gnunet-service-core.sock
UNIX_MATCH_UID = NO
UNIX_MATCH_GID = YES
# DISABLE_SOCKET_FORWARDING = NO
# USERNAME =
# MAXBUF =
# TIMEOUT =
# DISABLEV6 =
# BINDTO =
# REJECT_FROM =
# REJECT_FROM6 =
# PREFIX =

# Note: this MUST be set to YES in production, only set to NO for testing
# for performance (testbed/cluster-scale use!).
USE_EPHEMERAL_KEYS = YES
