#ifndef GNUNET_CONFIG_H
#define GNUNET_CONFIG_H

/**
 * @author Martin Schanzenbach
 *
 * @file
 * Convenience header including public (!) information on the
 * installed GNUnet configuration.
 *
 */

/* The version of GNUnet */
#define GNUNET_VERSION "@gnunet_version@"

/* Major version */
#define GNUNET_MAJOR_VERSION @gnunet_major_version@

/* Micro version */
#define GNUNET_MINOR_VERSION @gnunet_minor_version@

/* Mico version */
#define GNUNET_MICRO_VERSION @gnunet_micro_version@

/* Set to 1 is this is an experimental build */
#define GNUNET_EXPERIMENTAL @enable_experimental@

/* Set to 1 if GnuTLS has DANE support */
#define GNUNET_CURL_GNUTLS @curl_gnutls@

/**
 * Set to 1 if file-sharing meta data parsing is built with
 * libextractor, which means that callers to the API must use also use
 * libextractor.
 */
#define GNUNET_FS_EXTRACTOR_ENABLED @extractor@

#endif
